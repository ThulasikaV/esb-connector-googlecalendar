<component name="googlecalendar-calendars" type="synapse/template">
    <subComponents>
        <component name="clearCalendar">
            <file>clearCalendar.xml</file>
            <description>Clears a primary calendar. This operation deletes all data associated with
                the primary calendar of an account and cannot be undone.
            </description>
        </component>
        <component name="deleteCalendar">
            <file>deleteCalendar.xml</file>
            <description>Deletes a secondary calendar.</description>
        </component>
        <component name="getCalendar">
            <file>getCalendar.xml</file>
            <description>Returns metadata for a calendar.</description>
        </component>
        <component name="createCalendar">
            <file>createCalendar.xml</file>
            <description>Creates a secondary calendar.</description>
        </component>
        <component name="patchCalendar">
            <file>patchCalendar.xml</file>
            <description>Updates metadata for a calendar. This method supports patch semantics.
            </description>
        </component>
        <component name="updateCalendar">
            <file>updateCalendar.xml</file>
            <description>Updates metadata for a calendar.</description>
        </component>
    </subComponents>
</component>