<component name="googlecalendar-events" type="synapse/template">
    <subComponents>
        <component name="deleteEvent">
            <file>deleteEvent.xml</file>
            <description>Deletes an event.</description>
        </component>
        <component name="getEvent">
            <file>getEvent.xml</file>
            <description>Returns an event.</description>
        </component>
        <component name="importEvents">
            <file>importEvents.xml</file>
            <description>Imports an event. This operation is used to add a private copy of an
                existing event to a calendar.
            </description>
        </component>
        <component name="createEvent">
            <file>createEvent.xml</file>
            <description>Creates an event.</description>
        </component>
        <component name="getEventInstances">
            <file>getEventInstances.xml</file>
            <description>Returns instances of the specified recurring event.</description>
        </component>
        <component name="listEvents">
            <file>listEvents.xml</file>
            <description>Returns events on the specified calendar.</description>
        </component>
        <component name="moveEvent">
            <file>moveEvent.xml</file>
            <description>Moves an event to another calendar, i.e. changes an event&apos;s
                organizer.
            </description>
        </component>
        <component name="patchEvents">
            <file>patchEvents.xml</file>
            <description>Updates an event. This method supports patch semantics.</description>
        </component>
        <component name="updateEvents">
            <file>updateEvents.xml</file>
            <description>Updates an event.</description>
        </component>
        <component name="createQuickAddEvents">
            <file>createQuickAddEvents.xml</file>
            <description>Creates an event based on a simple text string.</description>
        </component>
    </subComponents>
</component>