<component name="googlecalendar-acl" type="synapse/template">
    <subComponents>
        <component name="deleteAcl">
            <file>deleteAcl.xml</file>
            <description>Deletes an access control rule.</description>
        </component>
        <component name="getAcl">
            <file>getAcl.xml</file>
            <description>Returns an access control rule.</description>
        </component>
        <component name="createAcl">
            <file>createAcl.xml</file>
            <description>Creates an access control rule.</description>
        </component>
        <component name="listAcl">
            <file>listAcl.xml</file>
            <description>Returns the rules in the access control list for the calendar.
            </description>
        </component>
        <component name="patchAcl">
            <file>patchAcl.xml</file>
            <description>Updates an access control rule. This method supports patch semantics.
            </description>
        </component>
        <component name="updateAcl">
            <file>updateAcl.xml</file>
            <description>Updates an access control rule.</description>
        </component>
    </subComponents>
</component>
